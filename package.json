{
  "name": "debugger-app",
  "version": "1.0.0",
  "description": "A Node.js application that consumes events from a Redis service, and displays them in a React UI.",
  "main": "src/server/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/calthomson/debugger-app"
  },
  "scripts": {
    "build": "webpack --mode production",
    "start": "npm run build && node src/server/index.js",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon src/server/index.js",
    "dev": "concurrently 'npm run server' 'npm run client'",
    "test": "jest --verbose --coverage"
  },
  "author": "Caledonia Thomson",
  "license": "ISC",
  "dependencies": {
    "evergreen-ui": "^3.2.7",
    "express": "^4.16.3",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "redis": "^2.8.0",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.0",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^4.0.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint": "^5.0.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "nodemon": "^1.17.3",
    "redis-mock": "^0.33.0",
    "regenerator-runtime": "^0.12.1",
    "stryker": "^0.30.1",
    "stryker-api": "^0.21.4",
    "stryker-javascript-mutator": "^0.11.2",
    "stryker-jest-runner": "^1.2.5",
    "url-loader": "^1.0.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.3"
  }
}
